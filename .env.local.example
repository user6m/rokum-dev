# Copy to .env.local and fill values
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
NEXTAUTH_SECRET=change_this_to_a_random_value
NEXTAUTH_URL=http://localhost:3000
# Comma-separated allowed emails for this app (only these can sign in)
ALLOWED_USERS=alice@example.com,bob@example.com

# In Google Cloud Console set these values for the OAuth client:
# Authorized JavaScript origin: http://localhost:3000
# Authorized redirect URI: http://localhost:3000/api/auth/callback/google
